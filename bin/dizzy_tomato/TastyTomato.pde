
void settings() {
  size(500, 500);
}
void draw() {
  background(200, 200, 200);
  noStroke();
  fill(150, 0, 0);
  ellipse(150, 200, 150, 150);
  ellipse(212, 200, 150, 150);
  fill(0, 200, 0);
  rect(176, 103, 12, 32);

if(mousePressed){
fill(200,200,200);
  ellipse(80, 200 ,30,30);
}

}



